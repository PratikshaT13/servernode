{
  "version": 2,
  "builds": [
    {
      "src": "pom.xml",
      "use": "@vercel/java",
      "config": {
        "buildCommand": "mvn clean package",
        "startCommand": "java -jar target/bus_tracker-0.0.1-SNAPSHOT.jar"
      }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/"
    }
  ]
}
